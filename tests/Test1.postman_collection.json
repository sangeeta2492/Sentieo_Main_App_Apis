{
	"info": {
		"_postman_id": "ed022b9f-a642-4897-99ab-7b06e00280d8",
		"name": "Test Collection",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "csrfToken",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "5934f1c0-93ed-4cf1-b270-5f273ce7a91d",
						"exec": [
							"var x = postman.getResponseCookie(\"csrftoken\").value;",
							"pm.environment.set(\"csrfmiddlewaretoken\", x);",
							" console.log(x);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{app_url}}/api/set_csrf_cookie/",
					"host": [
						"{{app_url}}"
					],
					"path": [
						"api",
						"set_csrf_cookie",
						""
					]
				}
			},
			"response": []
		},
		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "32f706ee-e8a1-44f0-859d-930ea8eb169a",
						"exec": [
							"console.log(\"Response body:  \" + responseBody);",
							"",
							"var reqBody = JSON.stringify(request.data);",
							"console.log(reqBody);",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Cookie",
						"value": "csrftoken={{csrfmiddlewaretoken}};app_version={{app_version}}; device_name={{iphone}}; ios_version={{OS_Version}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "email",
							"value": "{{email}}",
							"type": "text"
						},
						{
							"key": "password",
							"value": "{{password}}",
							"type": "text"
						},
						{
							"key": "csrfmiddlewaretoken",
							"value": "{{csrfmiddlewaretoken}}",
							"type": "text"
						},
						{
							"key": "loc",
							"value": "{{loc_ios}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{user_app_url}}/api/login_1/",
					"host": [
						"{{user_app_url}}"
					],
					"path": [
						"api",
						"login_1",
						""
					]
				}
			},
			"response": []
		}
	],
	"protocolProfileBehavior": {}
}